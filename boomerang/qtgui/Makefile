# Makefile for building gui
# Generated by tmake at 17:58, 2003/09/12
#     Project: gui
#    Template: app
#############################################################################

#   #   #   #   #   #   #   #   #   #   #   #   #   #   #   #   #   #   #
#                                                                       #
#   Note: requires environment var QTGUI to point to QT installation;   #
#   to run the GUI you need QTGUI/lib in your LD_LIBRARY_PATH, and      #
#   of course an X server                                               #
#                                                                       #
#   #   #   #   #   #   #   #   #   #   #   #   #   #   #   #   #   #   #

####### Compiler, tools and options

CC	=	gcc
CXX	=	g++
CFLAGS	=	-Wall -W -O2 -DNO_DEBUG
CXXFLAGS=	-Wall -W -O2 -DNO_DEBUG
INCPATH	=	-I../include -I$(QTDIR)/include
LINK	=	g++
LFLAGS	=	
LIBS	=	$(SUBLIBS) -L$(QTDIR)/lib -L/usr/openwin/lib -lqt -lXext -lX11 -lm -lresolv -lsocket -lnsl
MOC	=	$(QTDIR)/bin/moc
UIC	=	$(QTDIR)/bin/uic

TAR	=	tar -cf
GZIP	=	gzip -9f

####### Files

HEADERS =	mainform.h \
		detailswidget.h \
		filemonitor.h \
		util.h
SOURCES =	main.cpp \
		mainform.cpp \
		detailswidget.cpp \
		filemonitor.cpp \
		../util/util.cpp
OBJECTS =	main.o \
		mainform.o \
		detailswidget.o \
		filemonitor.o \
		../util/util.o
INTERFACES =	
UICDECLS =	
UICIMPLS =	
SRCMOC	=	moc_mainform.cpp \
		moc_detailswidget.cpp \
		moc_filemonitor.cpp
OBJMOC	=	moc_mainform.o \
		moc_detailswidget.o \
		moc_filemonitor.o
DIST	=	
TARGET	=	gui
INTERFACE_DECL_PATH = .

####### Implicit rules

.SUFFIXES: .cpp .cxx .cc .C .c

.cpp.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cxx.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cc.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.C.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.c.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

####### Build rules


all: $(TARGET)

$(TARGET): $(UICDECLS) $(OBJECTS) $(OBJMOC) 
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS)

moc: $(SRCMOC)

tmake:
	tmake gui.pro

dist:
	$(TAR) gui.tar gui.pro $(SOURCES) $(HEADERS) $(INTERFACES) $(DIST)
	$(GZIP) gui.tar

clean:
	-rm -f $(OBJECTS) $(OBJMOC) $(SRCMOC) $(UICIMPLS) $(UICDECLS) $(TARGET)
	-rm -f *~ core

####### Sub-libraries


###### Combined headers


####### Compile

main.o: main.cpp \
		mainform.h \
		filemonitor.h

mainform.o: mainform.cpp \
		mainform.h \
		detailswidget.h \
		filemonitor.h

detailswidget.o: detailswidget.cpp \
		detailswidget.h

filemonitor.o: filemonitor.cpp \
		filemonitor.h

../util/util.o: ../util/util.cpp

moc_mainform.o: moc_mainform.cpp \
		mainform.h

moc_detailswidget.o: moc_detailswidget.cpp \
		detailswidget.h

moc_filemonitor.o: moc_filemonitor.cpp \
		filemonitor.h

moc_mainform.cpp: mainform.h
	$(MOC) mainform.h -o moc_mainform.cpp

moc_detailswidget.cpp: detailswidget.h
	$(MOC) detailswidget.h -o moc_detailswidget.cpp

moc_filemonitor.cpp: filemonitor.h
	$(MOC) filemonitor.h -o moc_filemonitor.cpp

