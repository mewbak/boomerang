#

# Automatic generation of dependencies
# automatic is better!
@MAKEDEPEND_TRUE@depend: $(MAKEDEPEND)
@MAKEDEPEND_TRUE@	$(MAKEDEPEND) -w120 -Y -I$(top_srcdir)/include -I. -I$(top_srcdir)/c -I$(top_srcdir)/codegen \
@MAKEDEPEND_TRUE@	-I$(top_srcdir)/frontend *.cpp *.c > /dev/null 2>&1

@MAKEDEPEND_TRUE@depend.in: $(MAKEDEPEND)
@MAKEDEPEND_TRUE@	$(MAKEDEPEND) -fMakefile.in -w120 -Y -I$(top_srcdir)/include -I. -I$(top_srcdir)/c -I$(top_srcdir)/codegen \
@MAKEDEPEND_TRUE@	-I$(top_srcdir)/frontend *.cpp *.c > /dev/null 2>&1
@MAKEDEPEND_FALSE@depend:
@MAKEDEPEND_FALSE@@echo You need the (X11) makedepend tool to generate dependencies
